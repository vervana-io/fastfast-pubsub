{
  "name": "fastfast-pubsub",
  "version": "1.0.3",
  "description": "NestJS microservice transport for AWS SQS and SNS with pub/sub patterns, featuring message acknowledgment, batch processing, retry logic, and SNS fan-out support.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.js.map"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vervana-io/fastfast-pubsub.git"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf lib/*.js lib/*.d.ts lib/*.js.map"
  },
  "keywords": [
    "nestjs",
    "aws",
    "sqs",
    "sns",
    "pubsub",
    "microservice",
    "transport",
    "messaging",
    "queue",
    "events"
  ],
  "author": "Akeeb Ismail",
  "license": "ISC",
  "devDependencies": {
    "@nestjs/testing": "^11.1.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@aws-sdk/client-sns": "^3.826.0",
    "@aws-sdk/client-sqs": "^3.826.0",
    "sqs-consumer": "^12.0.0",
    "sqs-producer": "^7.0.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.1.3",
    "@nestjs/microservices": "^11.1.3"
  }
}
